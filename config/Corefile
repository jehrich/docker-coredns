ehrich.home:53 {
    log . {
        class denial error
    }
    errors
    reload 10s
    hosts /etc/coredns/ehrich.home.hosts ehrich.home {
        192.168.3.1 gw.ehrich.home
        fallthrough
    }
    forward ehrich.home 192.168.3.2 192.168.3.3
    cache 120
}

.:53 {
    log . {
        class denial error
    }
    errors
    reload 10s
    forward . 8.8.8.8 9.9.9.9
    cache 120
}

